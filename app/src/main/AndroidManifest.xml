<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.learnproject">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
      android:usesCleartextTraffic="true"
      android:name=".LearnApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/Theme.LearnProject">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".frame.FrameActivity" />
        <activity android:name=".frame.mvp.MVPActivity" />
        <activity android:name=".view.CusViewActivity" />
        <activity android:name=".view.cover.CoverViewActivity" />
    </application>

</manifest>